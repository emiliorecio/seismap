<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
  <rule>
    <from>^(/jsp/.*\.jsp)$</from>
    <to last="true" type="forward">$1</to>
  </rule>
  <rule>
    <from>^(.*)$</from>
    <to type="passthrough">/site$1</to>
  </rule>
</urlrewrite>
