<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
  PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
  "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<urlrewrite>
  <rule>
    <from>^(/jsp/.*\.jsp)$</from>
    <to last="true" type="forward">$1</to>
  </rule>
  <rule>
    <from>^(.*)$</from>
    <to type="passthrough">/site$1</to>
  </rule>
</urlrewrite>
